{
    "sourceMaps": true,
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-runtime"
    ],
    "presets": [
        ["@babel/preset-env", {
            "modules": false,
            "targets": "> 0.25%, not dead",
            "useBuiltIns": "usage",
            "corejs": 3
        }],
        "@babel/preset-typescript"
    ]
}
